     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;FUNCTIONALITY&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>*</TDFORMAT>
       <TDLINE>In this method you can determine if certain fields are shown as</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>&apos;selected&apos; by default, or whether certain fields are always shown as</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>&apos;read-only&apos; (and therefore the selection option is not changeable), on</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>the MDG HANA Search View Generation UI.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;PRECONDITION&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;RESULT&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;PARAMETERS&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>IV_MODEL: Name of the data model for which the HANA search view is being</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>generated</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
